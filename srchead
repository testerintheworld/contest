<script>
    function changeClass() {
        $(this).prev().toggleClass('active')
    }
    $(function(){
        $('#contact').hover(function(){
            $('#settcontact').slideToggle('fast', changeClass);
            },
            function(){
            $('#settcontact').slideToggle('fast', changeClass);
        });
    });
    $('#settcontact').width($('name').width())
</script>
